{
  "name": "cypress-testing-automation",
  "version": "1.0.0",
  "description": "Comprehensive web testing framework using Cypress for demonstrating modern test automation capabilities",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "test:ui": "cypress run --spec 'cypress/e2e/ui/**/*.cy.js'",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*.cy.js'",
    "test:integration": "cypress run --spec 'cypress/e2e/integration/**/*.cy.js'",
    "test:headed": "cypress run --headed",
    "test:chrome": "cypress run --browser chrome --headed",
    "test:firefox": "cypress run --browser firefox --headed",
    "test:edge": "cypress run --browser edge --headed",
    "test:record": "cypress run --record --key YOUR_RECORD_KEY",
    "test:parallel": "cypress run --record --parallel --key YOUR_RECORD_KEY",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "report:mochawesome": "mochawesome-merge cypress/reports/*.json > cypress/reports/mochawesome.json && marge cypress/reports/mochawesome.json --reportDir cypress/reports --inline",
    "lint": "eslint cypress/**/*.js",
    "lint:fix": "eslint cypress/**/*.js --fix",
    "clean": "rm -rf cypress/screenshots cypress/videos allure-results allure-report cypress/reports"
  },
  "keywords": [
    "cypress",
    "testing",
    "automation",
    "e2e",
    "api-testing",
    "web-testing",
    "javascript",
    "test-automation"
  ],
  "author": "Mahad Siddiqui <mahadsiddiqui@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.0",
    "cypress-mochawesome-reporter": "^3.5.0",
    "cypress-allure-plugin": "^2.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-xpath": "^2.0.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-real-events": "^1.7.6",
    "cypress-wait-until": "^1.7.2",
    "cypress-iframe": "^1.0.1",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-downloadfile": "^1.2.3",
    "cypress-localstorage-commands": "^2.0.2",
    "cypress-network-idle": "^1.0.0",
    "cypress-promise": "^1.1.0",
    "cypress-axe": "^1.4.0",
    "cypress-multi-reporters": "^1.6.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "marge": "^5.0.0",
    "allure-commandline": "^2.24.0",
    "eslint": "^8.50.0",
    "eslint-plugin-cypress": "^2.15.1",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "chai": "^4.3.7",
    "chai-json-schema": "^1.5.1",
    "faker": "^6.6.6",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/support/step_definitions/"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
